{
  "name": "everyparser.js",
  "version": "1.0.0",
  "description": "Parser for everything in your life!",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jestconfig.json",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoramStiel/EveryParser.JS.git"
  },
  "keywords": [
    "parser",
    "math",
    "mathparser",
    "calculation",
    "string"
  ],
  "author": "Andreas, Burbach, Joram Stiel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoramStiel/EveryParser.JS/issues"
  },
  "homepage": "https://github.com/JoramStiel/EveryParser.JS#readme",
  "dependencies": {
    "@types/antlr4": "^4.7.2",
    "antlr4": "^4.10.1",
    "mathjs": "^10.5.3"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  }
}
